{
  "name": "@adexchange/aeg-common",
  "version": "0.0.6",
  "description": "Common classes",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "author": "Justin Durham",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdExchangeGrp/aeg-common.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "test": "mocha --recursive --compilers ts:ts-node/register test/**/*.ts",
    "test:coverage": "istanbul cover _mocha -- --recursive --compilers ts:ts-node/register test/**/*.ts",
    "lint": "tslint src/**/*.ts",
    "lint:watch": "npm run lint && onchange \"**/*.ts\" \"tslint.json\" -- npm run lint",
    "compile": "tsc",
    "build": "npm run lint && npm run compile && istanbul cover _mocha -- --recursive --compilers ts:ts-node/register --reporter mocha-junit-reporter test/unit/**/*.ts",
    "patch": "git reset --hard HEAD && git clean -fd && npm run compile && npm version patch -m \"[ci skip] %s\" && git push origin master && git push origin master --tags && npm publish"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/node": "^6.0.82",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.13.0",
    "onchange": "^3.2.1",
    "parallelshell": "^2.0.0",
    "should": "^11.2.0",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  },
  "dependencies": {
    "@adexchange/aeg-logger": "0.0.41",
    "async": "^2.1.5",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.4.7",
    "lodash": "^4.17.4",
    "moment-timezone": "^0.5.11"
  },
  "engine": "node >= 6.10.0"
}
